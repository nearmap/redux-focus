    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "redux": "^3.5.2"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --inline --dev",
    "clean": "rimraf ./build",
    "test": "run-s -s lint jest",
    "ci": "run-s clean test",
    "build": "run-s compile pkg:*",
    "compile": "babel -s --out-dir build/pkg src",
    "pkg:pkg": "cp package.json build/pkg/",
    "pkg:docs": "cp README.md build/pkg/",
    "lint": "run-s -s lint:*",
    "lint:js": "eslint --ignore-path .gitignore .",
    "jest": "jest --no-cache --verbose --config ./.jestrc",
    "coveralls": "cat build/cov/lcov.info | coveralls"
  }
