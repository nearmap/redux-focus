    "@babel/preset-env": "^7.0.0-beta.41",
    "@babel/preset-react": "^7.0.0-beta.41",
    "@babel/preset-stage-2": "^7.0.0-beta.41",
    "@babel/runtime": "^7.0.0-beta.41",
    "aws-sdk": "^2.207.0",
