{
  "name": "refocus",
  "version": "0.5.0",
  "license": "MIT",
  "description": "Redux enhancer for managing element focus",
  "repository": "https://github.com/nearmap/refocus",
  "bugs": "https://github.com/nearmap/refocus/issues",
  "homepage": "https://github.com/nearmap/refocus",
  "keywords": [
    "redux",
    "focus"
  ],
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf ./build",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:md": "remark -i .gitignore --no-stdout --use remark-lint *.md",
    "jest": "jest --runInBand --no-cache --verbose --config ./jest.config.json",
    "lint": "run-s -s lint:*",
    "clean-build": "run-s clean build",
    "build": "run-s clean build:* pkg:*",
    "build:lib": "babel src --verbose --source-maps --out-dir build/pkg",
    "build:example": "webpack --bail",
    "pkg:pkg": "cp package.json build/pkg/",
    "pkg:docs": "cp README.md build/pkg/",
    "test": "run-s -s lint jest",
    "preci": "npm --loglevel=warn install",
    "ci": "run-s -s clean test build"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.2",
    "@babel/core": "^7.0.0-rc.2",
    "@babel/node": "^7.0.0-rc.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.2",
    "@babel/plugin-proposal-decorators": "^7.0.0-rc.2",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0-rc.2",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0-rc.2",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0-rc.2",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.2",
    "@babel/polyfill": "^7.0.0-rc.2",
    "@babel/preset-env": "^7.0.0-rc.2",
    "@babel/preset-react": "^7.0.0-rc.2",
    "@babel/runtime": "^7.0.0-rc.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.0.2",
    "aws-sdk": "^2.299.0",
    "babel-loader": "^8.0.0-beta.6",
    "coveralls": "^3.0.2",
    "eslint": "^5.4.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^23.5.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-inspector": "^0.11.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "remark-cli": "~5.0.0",
    "remark-lint": "~6.0.1"
  },
  "dependencies": {
    "redux": "^4.0.0"
  }
}
